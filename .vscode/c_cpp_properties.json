{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/libEmbeddedTests/inc",
                "${workspaceFolder}/squantorLibEmbedded/inc",
                "${workspaceFolder}/squantorMinUnit/inc"
            ],
            "defines": [
                "DEBUG",
                "MINUNIT_MAX_TESTS=1000"
