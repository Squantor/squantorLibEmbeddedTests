{
    "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/squantorLibEmbeddedTests/debug/squantorLibEmbeddedTests.elf",
      "args": [],
      //"stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "preLaunchTask": "build",
      "setupCommands": 
      [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "debug CortexM LibEmbedded routines",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/bin/squantorLibEmbeddedCortexMTests/debug/squantorLibEmbeddedCortexMTests.elf",
