{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build PC based Lib Embedded Tests",
            "type": "shell",
            "command": "make -j20 PROJECT=squantorLibEmbeddedTests",
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "build CortexM LibEmbedded Tests",
            "type": "shell",
            "command": "make -j20 PROJECT=squantorLibEmbeddedCortexMTests",
            "group": "build",
            "problemMatcher": "$gcc"
        },
        {
            "label": "clean",
            "type": "shell",
            "command": "make clean",
            "group": "build",
            "problemMatcher": "$gcc"
        }
    ]
}