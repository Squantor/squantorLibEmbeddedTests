  "version": "2.0.0",
  "tasks": [
    {
      "label": "build libEmbeddedTests",
      "type": "shell",
      "command": "compiledb make -j28 PROJECT=libEmbeddedTests",
      "group": "build",
      "problemMatcher": "$gcc"
    },
    {
      "label": "build CortexM LibEmbedded Tests",
      "type": "shell",
      "command": "compiledb make -j28 PROJECT=squantorLibEmbeddedCortexMTests",
      "group": "build",
      "problemMatcher": "$gcc"
    },
    {
      "label": "clean",
      "type": "shell",
      "command": "make clean",
      "group": "build",
      "problemMatcher": "$gcc"
    }
  ]
}